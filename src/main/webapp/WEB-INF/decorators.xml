<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/decorators">

	<!-- Apply web template -->
	<decorator name="web" page="web.jsp">
		<pattern>/*</pattern>
	</decorator>

	<!-- Apply auth template -->
	<decorator name="auth" page="auth.jsp">
		<pattern>/auth*</pattern>
	</decorator>
</decorators>